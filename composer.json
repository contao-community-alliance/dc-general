{
    "name": "contao-community-alliance/dc-general",
    "description": "Universal data container for Contao",
    "keywords": [
        "framework",
        "data",
        "container",
        "driver",
        "php",
        "contao"
    ],
    "type": "contao-module",
    "homepage": "http://c-c-a.org/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name": "Stefan Heimes",
            "email": "stefan_heimes@hotmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/contao-community-alliance/dc-general/issues",
        "wiki": "http://de.contaowiki.org/DC_General",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/contao-community-alliance/dc-general"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core": "^3.5.5",
        "contao-community-alliance/composer-plugin": "^2.4",
        "contao-community-alliance/event-dispatcher": "^1.3",
        "contao-community-alliance/translator": "^2.0",
        "contao-community-alliance/events-contao-bindings": "^3.5.0",
        "contao-community-alliance/url-builder": "^1.3",
        "symfony/expression-language": "^2.4 || ^3.4",
        "ext-json": "*"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.2",
        "contao-community-alliance/contao-twig": "^1.12",
        "menatwork/contao-multicolumnwizard": "^3.3"
    },
    "replace": {
        "metamodels/dc_general": "self.version"
    },
    "autoload": {
        "classmap": [
            "contao-compat/DC_General.php"
        ],
        "psr-0": {
            "ContaoCommunityAlliance\\DcGeneral\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "ContaoCommunityAlliance\\DcGeneral\\": "tests"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/dc-general"
            }
        },
        "branch-alias": {
            "dev-develop": "2.0.x-dev"
        }
    }
}
