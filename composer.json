{
    "name": "contao-community-alliance/dc-general",
    "description": "Universal data container for Contao",
    "keywords": [
        "framework",
        "data",
        "container",
        "driver",
        "php",
        "contao"
    ],
    "type": "contao-bundle",
    "homepage": "http://c-c-a.org/",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name": "Stefan Heimes",
            "email": "stefan_heimes@hotmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/contao-community-alliance/dc-general/issues",
        "wiki": "http://de.contaowiki.org/DC_General",
        "irc": "irc://irc.freenode.org/contao.dev",
        "source": "https://github.com/contao-community-alliance/dc-general"
    },
    "require": {
        "php": ">=5.6",
        "contao/core-bundle": "~4.4",
        "contao-community-alliance/translator": "~2.0@dev",
        "contao-community-alliance/dependency-container": "~2.0@dev",
        "contao-community-alliance/events-contao-bindings": "~4.0@dev",
        "contao-community-alliance/url-builder": "~1.3@dev",
        "symfony/event-dispatcher": "~3.0",
        "symfony/expression-language": "~3.0"
    },
    "require-dev": {
       "phpcq/all-tasks": "~1.1",
        "contao/manager-bundle": "~4.4"
    },
    "autoload": {
        "classmap": [
            "src/Resources/contao-compat/DC_General.php"
        ],
        "psr-4": {
            "ContaoCommunityAlliance\\DcGeneral\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ContaoCommunityAlliance\\DcGeneral\\Test\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "ContaoCommunityAlliance\\DcGeneral\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-feature/contao4": "2.1.x-dev"
        }
    }
}
